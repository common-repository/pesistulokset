{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "pesistulokset/tilastot",
  "version": "1.0.0",
  "title": "Pesistilastot",
  "category": "embed",
  "attributes": {
    "blockId": {
      "type": "string",
      "default": ""
    },
    "season": {
      "type": "string",
      "default": "2023"
    },
    "level": {
      "type": "string",
      "default": "Superpesis"
    },
    "series": {
      "type": "string",
      "default": "Miehet"
    },
    "group": {
      "type": "string",
      "default": "Runkosarja"
    },
    "phase": {
      "type": "string",
      "default": "Runkosarja"
    },
    "region": {
      "type": "string",
      "default": ""
    },
    "type": {
      "type": "string",
      "default": "pelaajat"
    },
    "stat": {
      "type": "string",
      "default": "lyodyt"
    },
    "team": {
      "type": "string",
      "default": ""
    }
  },
  "icon": "star-filled",
  "description": "Näyttää pelaajien tai joukkueiden tilastot.",
  "example": {},
  "supports": {
    "html": false,
    "align": true,
    "color": {
      "background": true,
      "text": true,
      "enableContrastChecker": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "__experimentalDefaultControls": {
        "margin": true,
        "padding": true
      }
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "__experimentalFontStyle": true,
      "__experimentalTextTransform": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalDefaultControls": {
        "fontSize": true
      }
    }
  },
  "textdomain": "pesistulokset",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./style-index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}