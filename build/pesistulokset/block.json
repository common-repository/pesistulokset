{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "pesistulokset/pesistulokset-block",
  "version": "1.0.0",
  "title": "Pesistulokset",
  "category": "embed",
  "attributes": {
    "blockId": {
      "type": "string",
      "default": ""
    },
    "season": {
      "type": "string",
      "default": "2023"
    },
    "level": {
      "type": "string",
      "default": "Superpesis"
    },
    "series": {
      "type": "string",
      "default": "Miehet"
    },
    "group": {
      "type": "string",
      "default": "Runkosarja"
    },
    "phase": {
      "type": "string",
      "default": "Runkosarja"
    },
    "region": {
      "type": "string",
      "default": ""
    },
    "highlight": {
      "type": "string",
      "default": ""
    },
    "highlightBgColor": {
      "type": "string",
      "default": ""
    },
    "headerBgColor": {
      "type": "string",
      "default": ""
    },
    "headerColor": {
      "type": "string",
      "default": ""
    },
    "highlightColor": {
      "type": "string",
      "default": ""
    },
    "customHeaderBgColor": {
      "type": "string",
      "default": "#0c71c3"
    },
    "customHeaderColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "customHighlightBgColor": {
      "type": "string",
      "default": "#d4edfd"
    },
    "customHighlightColor": {
      "type": "string",
      "default": ""
    },
    "showHeader": {
      "type": "boolean",
      "default": true
    },
    "columns": {
      "type": "array",
      "default": [
        "O",
        "V",
        "T",
        "H",
        "3P",
        "2P",
        "1P",
        "0",
        "J",
        "P"
      ]
    }
  },
  "icon": "star-filled",
  "description": "Näyttää pesiksen sarjataulukon.",
  "example": {},
  "supports": {
    "html": false,
    "align": true,
    "color": {
      "background": true,
      "text": true,
      "enableContrastChecker": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "__experimentalDefaultControls": {
        "margin": true,
        "padding": true
      }
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "__experimentalFontStyle": true,
      "__experimentalTextTransform": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalDefaultControls": {
        "fontSize": true
      }
    }
  },
  "textdomain": "pesistulokset",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./style-index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}